#include<stdio.h>
int main()
{
	int a[]={24,36,48,52,108,126,72};
	int i,target=108,count =0,temp;
	int n=sizeof(a)/sizeof(a[0]);
	for(i=0;i<n;i++)
	{
		if(a[i]==target)
		{
	      count=1;
	      temp=i;
        }
        
	}
	if(count==1)
	{
		printf("target found at position %d",temp);
	}
	else
	printf("target not found");
}
